local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Mini hub (tha hood)", HidePremium = false, IntroEnabled = false, SaveConfig = true, ConfigFolder = "MiniHubConfig"})



--tabs

local ThaTab = Window:MakeTab({
	Name = "Tha hood",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local MiscTab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local CreditsTab = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

-- tha hood scripts

ThaTab:AddLabel("Script made by cntrler V2#9768")

ThaTab:AddButton({
	Name = "Silent aim :D",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Jake1236552/scritps-for-me/main/tha%20hood"))()
  	end   
})

ThaTab:AddButton({
	Name = "Silent aim/camlock",
	Callback = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/p8DLK41k"))()
  	end   
})

ThaTab:AddButton({
	Name = "Tha hood GUI",
	Callback = function()
        loadstring(game:HttpGet("https://gist.githubusercontent.com/Shag420/5e43ec40b57ee329e4aa775026ba4ee7/raw"))()
  	end   
})

ThaTab:AddToggle({
	Name = "Auto-Max Armor : $3200 Per Refill",
	Default = false,
	Callback = function(Value)
		getgenv().autoarmor = Value
while autoarmor do
task.wait()
if game:GetService("Players").LocalPlayer.Armor.Value ~= 100 then
if #game.Players.LocalPlayer.Backpack:GetChildren() ~= 0 then
if game:GetService("Workspace").Folder.Armor:FindFirstChild("[Full Armor] - $3200") then
fireclickdetector(game:GetService("Workspace").Folder.Armor:FindFirstChild("[Full Armor] - $3200").Head.ClickDetector)
else
fireclickdetector(game:GetService("Workspace").Folder.Armor:FindFirstChild("***DONT SPAM ARMOR*** - $3200").Head.ClickDetector)
end
end
end
end
	end    
})

-- misc tab

MiscTab:AddButton({
	Name = "Infinite yeild",
	Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
  	end   
})



-- credits

CreditsTab:AddLabel("Made by cntrler V2#9768")

CreditsTab:AddLabel("Script testers â†“")

CreditsTab:AddLabel("3t1nn#8006")

CreditsTab:AddLabel("godhand#0443")

OrionLib:Init()
